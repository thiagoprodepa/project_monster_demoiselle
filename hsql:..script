CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE BOOKMARK(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DESCRIPTION VARCHAR(255),LINK VARCHAR(255))
CREATE MEMORY TABLE PROJETO(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DESCRICAO VARCHAR(255),NOME VARCHAR(255),ID_LIDER BIGINT,ID_SETOR BIGINT)
CREATE MEMORY TABLE USUARIO(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,MATRICULA VARCHAR(255),NOME VARCHAR(255),PERFIL INTEGER,ID_SETOR BIGINT)
CREATE MEMORY TABLE PROJETO_DESENVS(PROJETO_ID BIGINT NOT NULL,DESENV_ID BIGINT NOT NULL,CONSTRAINT SYS_CT_74 UNIQUE(DESENV_ID),CONSTRAINT FK21AAE11C82ECC94E FOREIGN KEY(DESENV_ID) REFERENCES USUARIO(ID),CONSTRAINT FK21AAE11CA74CBCBB FOREIGN KEY(PROJETO_ID) REFERENCES PROJETO(ID))
CREATE MEMORY TABLE SETOR(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NOME VARCHAR(255),CONSTRAINT SYS_CT_77 UNIQUE(NOME))
ALTER TABLE PROJETO ADD CONSTRAINT FK50C8E5035572E987 FOREIGN KEY(ID_SETOR) REFERENCES SETOR(ID)
ALTER TABLE PROJETO ADD CONSTRAINT FK50C8E50313D98E5F FOREIGN KEY(ID_LIDER) REFERENCES USUARIO(ID)
ALTER TABLE USUARIO ADD CONSTRAINT FK5B4D8B0E5572E987 FOREIGN KEY(ID_SETOR) REFERENCES SETOR(ID)
ALTER TABLE BOOKMARK ALTER COLUMN ID RESTART WITH 11
ALTER TABLE PROJETO ALTER COLUMN ID RESTART WITH 1
ALTER TABLE USUARIO ALTER COLUMN ID RESTART WITH 1
ALTER TABLE SETOR ALTER COLUMN ID RESTART WITH 7
